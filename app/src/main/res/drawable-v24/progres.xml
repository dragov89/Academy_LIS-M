<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#9C1816">
    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="#305438"
                    android:endColor="#1ac742"
                    android:angle="270" />
            </shape>
        </clip>
    </item>



    <item
        android:bottom="0dp"
    android:left="0dp"
    android:right="0dp"
    android:top="0dp">
    <shape>
        <corners
            android:radius="5dip" />
        <stroke
            android:width="2dp"
        android:color="#59595E" />
    </shape>
</item>
</layer-list>